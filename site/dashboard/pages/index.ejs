<!-- views/pages/about.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../partials/head %>
</head>

<header>
    <% include ../partials/header %>
</header>
<div class="bg" style="min-height: 100vh;">
    <br>
    
    <div class="table-responsive" style="border: none;">

    <table style="margin-top: 1rem;width: 90%;  margin: auto;text-align: center;" class="table table-hover table-dark h5 table-curved">
        <thead>
            <tr style="border-top: none;">
                <th scope="col">#</th>
                <th scope="col">Discord Name</th>
                <th scope="col">Discord ID</th>
                <th scope="col">Next Payment Date</th>
                <th scope="col">Subscription ID</th>
                <th scope="col">Customer ID</th>
                <th scope="col">Kick</th>
            </tr>
        </thead>
        <tbody>
        <% values.forEach(function(value) { %>
        <tr>
            <th scope="row"><%= value.index %></th>
            <th>Placeholder</th>
            <th><%= value.discord_id %></th>
            <th><%= value.next_payment %></th>
            <th><%= value.sub_id %></th>
            <th><%= value.cust_id %></th>
            <th><%=  %></th>
        </tr>
        <% }); %>
        </tbody>
    </table>
    </div>
    <% include ../partials/footer %>
</div>
</html>
